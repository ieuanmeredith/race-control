{
  "extends": "tslint:recommended",
  "rules": {
    "directive-selector": [
      true,
      "attribute",
      "app",
      "camelCase"
    ],
    "no-console": [
      true,
      "trace"
    ],
    "component-selector": [
      true,
      "element",
      "app",
      "kebab-case"
    ]
  }
}
